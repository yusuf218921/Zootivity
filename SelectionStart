using System.Collections;
using System.Collections.Generic;
using UnityEngine;
using DG.Tweening;

public class SelectionStart : MonoBehaviour
{
    [SerializeField]
    private Transform body, head, countine;
    
    void Start()
    {
        body.transform.GetComponent<RectTransform>().DOLocalMoveX(650f, 1f).SetEase(Ease.OutBack);
        head.transform.GetComponent<RectTransform>().DOLocalMoveX(650f, 1f).SetEase(Ease.OutBack);
        countine.transform.GetComponent<RectTransform>().DOLocalMoveX(650f, 1f).SetEase(Ease.OutBack);
    }

    
    
}
